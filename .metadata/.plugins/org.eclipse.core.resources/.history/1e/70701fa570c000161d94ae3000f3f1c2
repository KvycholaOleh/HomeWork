package except;

public class Main {

	public static void main(String[] args) throws IllegalAccessException, MyException {
		try {
			Methods.add(-1, -1);
		} catch (IllegalArgumentException e) {
		}
		
		try {
			Methods.sub(0, 1);
		} catch (ArithmeticException e) {
		}
		
		Methods.mult(0, 0);
		
		
		
		
		Methods.div(1, 1);
		
	}

}
