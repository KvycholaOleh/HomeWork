package strObj;

public class Rectangle {
	int width;
	int height;
	
	Rectangle(){
		
	}
	
	Rectangle(int width, int height){
		this.width=width;
		this.height=height;
	}
	
	int perimeter(int width, int height){
		return ((width+height)*2);
	}
	
	double area(int width, int height){
		return width*height;
	}

}
